[
  {
    "description":"无参mock,返回cookie",
    "request":{
      "uri":"/getNoParamDemo/returnCookie",
      "method":"get"
    },
    "response":{
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      },
      "cookies":{
        "login":"true"
      },
      "text":"获取到cookie信息"
    }
  },
  {
    "description":"模拟一个post请求",
    "request":{
      "uri":"/postParamDemo/cookie",
      "method":"post",
      "cookies":{
        "login":"true"
      },
      "headers":{
        "Content-Type":"application/json"
      },
      "json":{
        "name":"huhansan",
        "age":"18"
      }
    },
    "response":{
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      },
      "status":200,
      "json":{
        "huhansan":"success",
        "status":"1"
      }
    }
  }
]